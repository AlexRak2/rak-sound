<Application x:Class="SonnissBrowser.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="Bg0" Color="#0B0F14"/>
        <SolidColorBrush x:Key="Bg1" Color="#0F1621"/>
        <SolidColorBrush x:Key="Bg2" Color="#111B28"/>
        <SolidColorBrush x:Key="Border" Color="#233144"/>
        <SolidColorBrush x:Key="Text0" Color="#E6EDF3"/>
        <SolidColorBrush x:Key="Text1" Color="#A8B3C2"/>
        <SolidColorBrush x:Key="Text2" Color="#7E8AA0"/>
        <SolidColorBrush x:Key="Accent" Color="#4C8DFF"/>
        <SolidColorBrush x:Key="Good" Color="#37D67A"/>
        <CornerRadius x:Key="Cr8">8</CornerRadius>
        <CornerRadius x:Key="Cr10">10</CornerRadius>
    </Application.Resources>
</Application>